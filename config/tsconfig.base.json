{
    "compilerOptions": {
        "composite": true,
        "module": "commonjs",
        "target": "es6",
        "strict": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true,
    },
    "exclude": [
        "node_modules"
    ],
}