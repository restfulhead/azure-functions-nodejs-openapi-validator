{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "strict": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "out",
        "sourceMap": true,
        "baseUrl": "./",
    },
    "include": [
        "src/**/*.ts"
    ],
}